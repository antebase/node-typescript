{
  "author": "github.com/dan-muller",
  "dependencies": {
    "express": "^4.17.1"
  },
  "description": "Antebase is comes before your app.",
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "jest": "^27.0.4",
    "prettier": "^2.3.1",
    "ts-node-dev": "^1.1.6",
    "tslint": "^6.1.3",
    "typescript": "^4.3.2"
  },
  "main": "index.ts",
  "name": "@antebase/node-typescript",
  "prettier": {
    "arrowParens": "always",
    "endOfLine": "auto",
    "bracketSpacing": true,
    "filepath": "src",
    "trailingComma": "all",
    "singleQuote": true,
    "tabWidth": 4,
    "semi": false,
    "useTabs": false
  },
  "scripts": {
    "dev:test": "jest",
    "dev:start": "ts-node-dev --respawn --transpile-only index.ts",
    "build": "tsc --build --verbose",
    "start": "tsc && node dist/index.js"
  },
  "version": "0.0.1"
}
